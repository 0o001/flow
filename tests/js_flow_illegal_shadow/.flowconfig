[options]
all=true
module.system=haste
module.system.haste.use_name_reducers=true
# get basename
module.system.haste.name_reducers='^.*/\([a-zA-Z0-9$_.-]+\.\(js\|tsx?\)\(\.flow\)?\)$' -> '\1'
module.system.haste.name_reducers='^\(.*\)\.\(js\|tsx?\)\(\.flow\)?$' -> '\1'
# strip .ios suffix
module.system.haste.name_reducers='^\(.*\)\.ios$' -> '\1'
experimental.multi_platform.extensions=.ios
